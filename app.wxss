@import './antui/antui.wxss';

page{
  height: 100%;
  overflow-x: hidden;
  }

/*********---guide-page-panel---***********/
.guide-page-panel{
    display: none;
    position: fixed;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    background: url(https://gw.alipayobjects.com/zos/rmsportal/TMVMvgtZLgtzzyHYGzHP.png) no-repeat right top;
    background-size: 100% 100%;
    z-index: 99999;
}
.guide-icon{
    width: 150px;
    height: 150px;
    background: url(https://gw.alipayobjects.com/zos/rmsportal/RNsGCrSEPcPCDmEhMVIr.png) no-repeat right top;
    background-size: 100% 100%;
    margin: 100px auto 0;
}
.guide-page-panel .header{
    color: #fff;
    text-align: center;
}
.guide-page-panel .header h1{
    font-size: 38px;
    line-height: 38px;
    font-weight: 400;
}
.guide-page-panel .header .header-info{
    font-size: 12px;
    line-height: 18px;
    margin-top: 15px;
}
.guide-page-panel .guide-footer{
    position: absolute;
    left: 0;
    bottom: 10px;
    color: #fff;
    font-size: 9px;
    width: 100%;
    text-align: center;
}


.demo-transparent-title{
    display: none;
    width: 100%;
    height: 65px;
    position: fixed;
    top: 0;
    left: 0;
    background: url(data:image/png;base64,) no-repeat right top;
    background-size: 100% 100%;
    z-index: 9999;
}

/*********---widget-list-panel---***********/
.widget-list-panel{
    position: relative;
    min-height: 100%;
    padding: 0 0 80px;
    box-sizing:border-box;
}
.widget-list-panel .header{
    position:relative;
    padding-bottom: 32px;
    color: #fff;
    text-align: center;
    z-index: 1;
}
.widget-list-panel .header-icon{
    display: block;
    width: 53px;
    height: 53px;
    background: url(https://gw.alipayobjects.com/zos/rmsportal/iOaNylpMIrSWlGcHZubp.png) no-repeat right top;
    background-size: 100% auto;
    margin: 50px auto 0;
}
.widget-list-panel .scenario-header-icon{
    background: url(https://gw.alipayobjects.com/zos/rmsportal/jbJODYNcsZLNHVvuilZK.png) no-repeat right top;
    background-size: auto 100%;
}
.widget-list-panel .header-info{
    font-size: 14px;
    line-height: 20px;
    padding-top: 16px;
    color: #888;
}

.widget-list{
    padding: 0px 15px 15px 0;
    list-style: none;
    box-sizing: border-box;
    width: 100%;
    max-width: 1200px;
    margin: 0 auto;
    position: relative;
    z-index: 1;
    min-height: 100%;
}
.widget-list .widget-item {
    list-style: none;
    width: 100%;
    padding: 0 0 15px 15px;
    box-sizing: border-box;
    border-radius: 3px;
}
.list-hide{
    display: none;
}
.widget-list .list-grey{
    opacity: .32;
}
.widget-list .widget-wrap-item{
    list-style: none;
    display: -webkit-box;
    width: 100%;
    height: 100%;
    background: rgba(255, 255, 255, 0.9);
    padding: 20px 15px;
    box-sizing: border-box;
}
.widget-list .icon-left-box{
    list-style: none;
    height: 22px;
    -webkit-box-flex:1;
}
.widget-list .icon-right-box{
    list-style: none;
    height: 22px;
    text-align: right;
    -webkit-box-flex:1;
}
.widget-list .name{
    -webkit-box-flex:14;
}
.widget-list .name span {
    float: right;
    font-size: 10px;
    border-radius: 200px;
    background: #108EE9;
    padding: 0 8px;
    color: #fff;
    margin-left: 10px;
    font-weight: 500;
}
.widget-list .widget-wrap-list{
    width: 100%;
    height: 100%;
    background: rgba(255, 255, 255, 0.9);
    padding: 0px 15px 0;
    box-sizing: border-box;
}
.widget-list .list-header{
    padding: 0;
}
.widget-list .widget-wrap{
    display: -webkit-box;
    width: 100%;
    height: 100%;
    border-bottom: rgba(238, 238, 238, 0.95) solid 1px;
    padding: 10px 0;
}
.widget-list .widget-wrap-a .widget-wrap-list:last-child .widget-wrap{
    border-bottom: 0;
}
.widget-list .widget-wrap-list:active{
    background: rgba(238, 238, 238, 0.95);
    border-radius: 3px;
}
.widget-list .icon{
    display: inline-block;
    width: 22px;
    height: 22px;
    background: url(https://gw.alipayobjects.com/zos/rmsportal/PnTecXTcIfZuObLndKrn.png) no-repeat right top;
    background-size: 22px auto;
}
.widget-list .icon.navigation{
    background-position: 0px -26px;
}
.widget-list .icon.search{
    background-position: 0px -87px;
}
.widget-list .icon.angle{
    background-position: 6px -1px;
}
.widget-list .icon.basis {
    background-position: 0px -55px;
}
.widget-list .icon.list {
    background-position: 0px -117px;
}
.widget-list .icon.result {
    background-position: 0px -146px;
}
.widget-list .icon.biz {
    background-position: 0px -240px;
}
.widget-list .icon.interface{
    background-position: 0px -341px;
}
.widget-list .icon.video{
    background-position: 0px -377px;
}
.widget-list .icon.skin{
    background-position: 0px -417px;
}
/******footer********/
.footer{
    list-style: none;
    display:table;
    table-layout: fixed;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 9999;
    height: 50px;
    width: 100%;
    background: rgba(255, 255, 255, 1);
    border: rgba(238, 238, 238, 0.95) solid 1px;
    font-size: 10px;
    color: #7A7E83;
}
.footer .item{
    display: inline-block;
    list-style: none;
    width: 50%;
    height: 100%;
    text-align: center;
}
.footer .icon{
    display: inline-block;
    width: 21px;
    height: 21px;
    background: url(https://gw.alipayobjects.com/zos/rmsportal/PnTecXTcIfZuObLndKrn.png) no-repeat right top;
    background-size: 21px auto;
    margin-top: 8px;
}
.footer .icon.comp{
    background-position: 0px -197px;
    /*background-position: 0px -257px;*/
}
.footer .icon.scen{
    background-position: 0px -170px;
    /*background-position: 0px -291px;*/
}
.footer .blue{
    color: #7FA5E8;
}

/*************************************/
.widget-list .name{
    font-size: 15px;
    color: #000;
    line-height: 22px;
    height: 22px;
    font-weight: 300;
}
.widget-list .info{
    font-size: 12px;
    color: #888;
}
.widget-demo-show-panel{
    background: #f5f5f5;
}

.demo{
    min-height: 100%;
    position: relative;
}

.demo-button .single-demo-button .am-button, .demo-tab .am-tab, .demo .am-message, .demo .am-notice, .demo .am-inform{
    margin-bottom: 10px;
}

.demo .am-message.multi,
.demo .am-button.bottom:last-child{
    margin-bottom: 0;
}
.demo .am-button.bottom{
    position: relative;
}

.demo .demo-content{
    padding-bottom: 150px;
}

.demo.demo-button .demo-content:last-child,
.demo.demo-loading .demo-content{
    padding-bottom: 0;
}

.demo-dialog .demo-content{
    height: 5rem;
    min-height: 500px;
    background: url(https://zos.alipayobjects.com/rmsportal/rTrIDRjzFvXhAfgdnPVD.jpg) no-repeat;
    background-size: 100% auto;
    position: relative;
    padding-bottom: 0;
}
.demo-dialog .demo-content .am-dialog, .demo-others .am-filter-button-wrap{
    position: absolute;
}
.demo-toast .demo-content, .demo-loading .demo-content{
    min-height: 300px;
    position: relative;
}
.demo-toast .am-toast{
    position: absolute;
}
.demo-page-result .demo-content{
    height: 5rem;
    min-height: 500px;
    padding-bottom: 0;
}
.demo-header{
    text-align: center;
    font-size: 15px;
    padding: 81px 0 21px;
    color: #000;
    opacity: .3;
    line-height: 50px;
}
.demo-header:after{
    content: " ";
    display: block;
    width: 47px;
    border-bottom: #000 solid 1px;
    margin: 0 auto;
}
.demo-header-brief{
    text-align: center;
    color: #888;
    font-size: 18px;
    padding-bottom: 32px;
    font-weight: 300;
}
.demo-type-title{
    padding: 0 15px 8px;
    font-size: 13px;
    font-weight: 300;
}
.demo-tab .demo-type-title,
.demo-list .demo-type-title,
.demo-search .demo-type-title,
.demo-notice .demo-type-title{
    color: #888;
    padding: 60px 15px 9px;
    display: inline-block;
    width: 100%;
    box-sizing: border-box;
}

.demo-others .demo-type-title{
    color: #000;
    padding: 60px 15px 10px;
    width: 100%;
    box-sizing: border-box;
    font-size: 20px;
}

.demo-popmenu .demo-type-title,
.demo-dialog .demo-type-title,
.demo-page-result .demo-type-title,
.demo-toast .demo-type-title,
.demo-loading .demo-type-title{
    margin-top: 50px;
    font-size: 15px;
    padding: 11px 15px;
    background-color: #3a3a3a;
    color: #fff;
    text-align: center;
    position: relative;
}

.demo-dialog .demo-type-title,
.demo-page-result .demo-type-title,
.demo-loading .demo-type-title{
    margin-top: 0;
}
.demo-popmenu .demo-type-title i.dot{
    position: absolute;
    right: 15px;
    top: 0px;
    font-size: 24px;
    line-height: 30px;
}
.demo-popmenu .demo-content{
    position: relative;
    min-height: 160px;
}
.demo-popmenu .am-popmenu-mask{
    position: absolute;
}

.demo-header-des{
    padding: 0 15px;
    font-size: 13px;
    color: #888;
}
.demo-article, .demo-agreement{
    background-color: #fff;
}

.am-demo-list-item{
    display: block;
    height: 44px;
    padding: 0 15px;
    box-sizing: border-box;
    line-height: 44px;
    font-size: 16px;
    color: #108ee9;
    background: -webkit-linear-gradient(to top, #ddd, #ddd 33%, transparent 33%) no-repeat left bottom #fff;
    background: linear-gradient(to top, #ddd, #ddd 33%, transparent 33%) no-repeat left bottom #fff;
    -webkit-background-size: 100% 1px;
    background-size: 100% 1px;
}
.am-demo-list-item:first-child {
    background: -webkit-linear-gradient(to bottom, #ddd, #ddd 33%, transparent 33%) no-repeat left top,
                -webkit-linear-gradient(to top, #ddd, #ddd 33%, transparent 33%) no-repeat left bottom #fff;
    background: linear-gradient(to bottom, #ddd, #ddd 33%, transparent 33%) no-repeat left top,
                linear-gradient(to top, #ddd, #ddd 33%, transparent 33%) no-repeat left bottom #fff;
    -webkit-background-size: 100% 1px, 100% 1px;
    background-size: 100% 1px, 100% 1px;
}

.demo-footer-box{
    position: absolute;
    height: 32px;
    width: 100%;
    bottom: 0;
    left: 0;
    text-align: center;
}
.demo-footer-box .demo-footer{
    /* width: 84px; */
    height: 36px;
    line-height: 18px;
    margin: 0 auto;
    background: url(data:image/png;base64,) no-repeat left top 2px;
    background-size: 30px 30px;
    padding-left: 40px;
    box-sizing:border-box;
    -webkit-transform: scale(0.5);
    -webkit-transform-origin: center center;
    display: inline-block;
}
.demo-footer-box .demo-footer p:first-child{
    font-size: 14px;
    color: #000;
    font-weight: 200;
}
.demo-footer-box .demo-footer p:last-child{
    font-size: 12px;
    color: #9197A3;
    font-weight: 200;
}
.am-bubble.demo-notice {
    position: absolute;
    top: 0;
    right: 0;
    margin:0;
    transform: translate(40%, -40%);
}
.demo-layout {
    margin: 0 10px;
}
.am-flexbox-item, .demo-layout-box{
    padding: 5px 0;
    background: #EBEBEB;
    text-align: center;
    font-size: 12px;
    font-weight: 300;
    color: #4A4A4A;
}

.am-flexbox-item + .am-flexbox-item, .am-flexbox-item + .demo-layout-box{
    margin-left: 10px;
}

.am-flexbox + .am-flexbox {
    margin-top: 10px;
}
.am-checkbox .demo-checkbox-label{
    color: #888888;
    font-size: 15px;
}
.demo-form-error .am-checkbox{
    margin: 0.05rem 0;
}
.demo-message-result{
    position: absolute;
    width: 100%;
    height: 100%;
}
.demo-message-result footer{
    position: absolute;
    bottom: 0;
}

.demo-swiper.swiper-container {
    height: 150px;
    margin:0 10px;
    overflow: hidden;
}
.demo-swiper + .demo-swiper{
    margin-top: 10px;
}
.demo-swiper .swiper-slide{
    line-height: 140px;
    text-align: center;
    background: #108EE9;
    color: #ffffff;
}

.demo-swiper.am-sp-white .swiper-slide{
    line-height: 140px;
    text-align: center;
    background: #ffffff;
    color: #4A4A4A;
}